{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "AllowS3OnlyPjCorridor",
			"Effect": "Allow",
			"Action": "s3:*",
			"Resource": [
				"arn:aws:s3:::pj-corridor.net",
				"arn:aws:s3:::pj-corridor.net/*"
			]
		},
		{
			"Sid": "AllowECROnlyPersonalitytest",
			"Effect": "Allow",
			"Action": "ecr:*",
			"Resource": "arn:aws:ecr:ap-northeast-1:256385929234:repository/personalitytest"
		},
		{
			"Sid": "AllowLambdaOnlyPersonalitytest",
			"Effect": "Allow",
			"Action": "lambda:*",
			"Resource": "arn:aws:lambda:ap-northeast-1:256385929234:function:personalitytest"
		},
		{
			"Sid": "AllowCloudFrontInvalidate",
			"Effect": "Allow",
			"Action": [
				"cloudfront:CreateInvalidation"
			],
			"Resource": "arn:aws:cloudfront::256385929234:distribution/E3MI1AH6ZMA0VR"
		},
		{
			"Sid": "ExplicitDenyAccountAndBilling",
			"Effect": "Deny",
			"Action": [
				"organizations:*",
				"account:*",
				"billing:*",
				"budgets:*",
				"cur:*",
				"ce:*"
			],
			"Resource": "*"
		},
		{
			"Sid": "DenyIAMExceptReadPolicy",
			"Effect": "Deny",
			"NotAction": [
				"iam:GetPolicy",
				"iam:GetPolicyVersion"
			],
			"Condition": {
				"StringEquals": {
					"aws:RequestedService": "iam.amazonaws.com"
				}
			},
			"Resource": "*"
		},
		{
			"Sid": "AllowReadOwnPolicies",
			"Effect": "Allow",
			"Action": [
				"iam:GetPolicy",
				"iam:GetPolicyVersion"
			],
			"Resource": [
				"arn:aws:iam::256385929234:policy/CIPolicyCorridorBoundary.json",
				"arn:aws:iam::256385929234:policy/AppPolicyPersonalitytest.json"
			]
		}
	]
}

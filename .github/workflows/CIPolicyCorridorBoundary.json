{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "UpperLimitOfS3",
			"Effect": "Allow",
			"Action": "s3:*",
			"Resource": [
				"arn:aws:s3:::pj-corridor.net",
				"arn:aws:s3:::pj-corridor.net/*"
			]
		},
		{
			"Sid": "UpperLimitOfLambda",
			"Effect": "Allow",
			"Action": "lambda:*",
			"Resource": "arn:aws:lambda:*:256385929234:function:personalitytest"
		},
		{
			"Sid": "UpperLimitOfCloudfront",
			"Effect": "Allow",
			"Action": [
				"cloudfront:CreateInvalidation"
			],
			"Resource": "arn:aws:cloudfront::256385929234:distribution/E3MI1AH6ZMA0VR"
		},
		{
			"Sid": "UpperLimitOfIAMReadOnly",
			"Effect": "Allow",
			"Action": [
				"iam:GetPolicy",
				"iam:GetPolicyVersion"
			],
			"Resource": [
				"arn:aws:iam::256385929234:policy/*"
			]
		},
		{
			"Sid": "AllowReadOnlyWAFv2",
			"Effect": "Allow",
			"Action": [
				"wafv2:ListWebACLs",
				"wafv2:GetWebACL"
			],
			"Resource": "*"
		},
		{
			"Sid": "OtherServicesAreAllDenied",
			"Effect": "Deny",
			"Action": [
				"organizations:*",
				"account:*",
				"billing:*",
				"budgets:*",
				"cur:*",
				"ce:*"
			],
			"Resource": "*"
		}
	]
}
